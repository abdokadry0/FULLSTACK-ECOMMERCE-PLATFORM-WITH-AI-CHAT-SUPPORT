# Frontend Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL="http://localhost:5000/api"

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key_here"

# App Configuration
REACT_APP_APP_NAME="Modern E-commerce Platform"
REACT_APP_ENVIRONMENT="development"

# Build Configuration
GENERATE_SOURCEMAP=false

# Analytics (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID="GA-XXXXXXXXX"

# Social Media Links (Optional)
REACT_APP_FACEBOOK_URL="https://facebook.com/yourpage"
REACT_APP_TWITTER_URL="https://twitter.com/yourhandle"
REACT_APP_INSTAGRAM_URL="https://instagram.com/yourhandle"

# Contact Information
REACT_APP_CONTACT_EMAIL="contact@yourstore.com"
REACT_APP_SUPPORT_PHONE="+1-555-123-4567"